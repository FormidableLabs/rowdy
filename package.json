{
  "name": "rowdy",
  "version": "0.1.4",
  "description": "WD.js configuration wrapper.",
  "main": "index.js",
  "dependencies": {
    "colors": "1.1.0",
    "lodash": "3.8.0"
  },
  "peerDependencies": {
    "selenium-standalone": ">=3.0.0",
    "wd": "*"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "eslint": "^0.20.0",
    "mocha": "^2.2.4",
    "phantomjs": "^1.9.16",
    "selenium-standalone": "^4.4.0",
    "wd": "^0.3.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/rowdy"
  },
  "keywords": [
    "functional test",
    "integration test",
    "test",
    "selenium",
    "mocha"
  ],
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/rowdy/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "check": "npm run lint",
    "check-all": "npm run lint && npm run test",
    "test": "npm run test-default-config && npm run test-manual-config",
    "test-default-config": "cd examples/mocha && mocha",
    "test-manual-config": "cd examples/mocha && mocha --require ./setup-manual-config",
    "install-selenium": "selenium-standalone install"
  }
}
