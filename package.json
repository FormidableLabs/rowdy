{
  "name": "rowdy",
  "version": "0.1.4",
  "description": "WD.js configuration wrapper.",
  "main": "index.js",
  "dependencies": {
    "colors": "0.6.2",
    "lodash": "2.4.1"
  },
  "peerDependencies": {
    "selenium-standalone": ">=3.0.0",
    "wd": "*"
  },
  "devDependencies": {
    "chai": "1.9.1",
    "gulp": "3.8.7",
    "gulp-jshint": "1.8.4",
    "mocha": "1.21.4",
    "phantomjs": "1.9.9",
    "selenium-standalone": "4.2.2",
    "wd": "0.3.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/rowdy"
  },
  "keywords": [
